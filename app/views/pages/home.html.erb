          <% end %>
           <%= link_to 'javascript:;', class: "healthcare sector-circule sector-effect" do %>
            <span class="overlay"></span>
            <span class="text">Healthcare</span>
          <% end %>
          <%= link_to 'javascript:;', class: "consumer sector-circule sector-effect", target: "_blank" do %>
            <span class="overlay"></span>
            <span class="text">Consumer</span>
          <% end %>
