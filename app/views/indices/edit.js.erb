$('tbody#pdf_attachments').html('<%= escape_javascript render(@pdf_attachments) %>');
$('#paginator_file').html('<%= escape_javascript(paginate(@pdf_attachments, :remote => true, param_name: "page_doc").to_s) %>');

$('.chosen_pdf_attachment').click(function() {
  $('#attached_file_id').val($(this).attr('id'));
  $.colorbox.close();
  $('.attached_file_pdf').attr('src', $(this).parent().parent().find('img').attr('src'));
  $('.remove_file').removeClass("hide");
});
