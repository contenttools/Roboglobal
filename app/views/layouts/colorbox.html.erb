<!DOCTYPE html>
<html>
  <head>
    <title>Robostox</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="root-container">
      <%= yield %>
    </div>
  </body>
</html>
<script type="text/javascript">
  <%= yield :js_end %>

  $(function() {
    <%= yield :js_init %>
  });
</script>
