<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => false %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => false %>
    <%= fetch_js(params) %>
    <%= csrf_meta_tags %>
    <%= display_meta_tags %>
    <script src="//use.typekit.net/qun2jvv.js"></script>
    <script>try{Typekit.load({ async: true });}catch(e){}</script>
    <link rel="shortcut icon" href="/favicon.ico">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>
    <%= render partial: "shared/main_header" %>
    <div id="root-container" class="main-shell">
      <%= render partial: "shared/flash_messages", flash: flash %>
      <%= yield %>
    </div>
    <%= render partial: "shared/main_footer" %>
    <%= render partial: "shared/leave_site_color_box_us" %>
    <%= render partial: "shared/leave_site_color_box_eu" %>
  </body>
</html>
<script type="text/javascript">
  <%= yield :js_end %>

  $(function() {
    <%= yield :js_init %>
  });

  <% unless ( signed_in? or current_page?(new_user_session_path)) %>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-68266335-1', 'auto');
    ga('send', 'pageview');
  <% end %>
</script>
